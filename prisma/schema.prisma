generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Curso {
  id     Int     @id @default(autoincrement())
  nome   String
  alunos Aluno[]
}

model Aluno {
  id             Int      @id @default(autoincrement())
  nome           String
  email          String   @unique
  cpf            String
  dataNascimento DateTime
  cursoId        Int

  curso Curso @relation(fields: [cursoId], references: [id])
}
